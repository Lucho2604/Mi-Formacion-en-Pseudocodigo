Algoritmo CicloPara
	
	//Bucle Para 
	Escribir "Contando de a uno";
	Para i=0 Hasta 10 Con Paso 1 Hacer
		Escribir i;
	FinPara
	
	Escribir "Contando de a dos";
	Para i=0 Hasta 10 Con Paso 2 Hacer
		Escribir i;
		
	FinPara
	
	Escribir "Contando en orden descendente";
	Para i=10 Hasta 0 Con Paso -1 Hacer
		Escribir i;
		
	FinPara
	
	
FinAlgoritmo
